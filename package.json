        "@payloadcms/next": "3.9.0",
        "@payloadcms/payload-cloud": "3.9.0",
        "@payloadcms/richtext-lexical": "3.9.0",
        "@web3forms/react": "^1.1.3",
        "cross-env": "^7.0.3",
        "graphql": "^16.8.1",
        "next": "15.1.0",
        "payload": "3.9.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
