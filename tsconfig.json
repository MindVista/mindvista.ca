{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@public/*": ["/public/*"],
            "@components/*": ["src/components/*"],
            "@layouts/*": ["src/layouts/*"],
            "@pages/*": ["src/pages/*"],
            "@schemas/*": ["src/schemas/*"],
            "@styles/*": ["src/styles/*"],
            "@utils/*": ["src/utils/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    },
}
